{
  "buildCommand":  "npm run build",
  "outputDirectory": ".next",
  "framework":     "nextjs",
  "regions":       ["sin1"],
  "headers": [
    {
      "source":  "/(.*)\\.(js|css|svg|png|jpg|jpeg|gif|ico|woff|woff2)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
